{
  "name": "tacohuaco",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "astro dev",
    "prebuild": "npm run sync",
    "build": "astro build",
    "preview": "astro preview",
    "pretest": "npm run lint",
    "test": "astro check && vitest run",
    "test:watch": "vitest watch",
    "test:ci": "npm run lint && vitest run",
    "posttest": "npm run format",
    "lint": "eslint . --cache --fix --ext .js,.ts,.tsx,.astro",
    "format": "prettier --log-level warn --write \"**/*.{js,ts,tsx,astro,md}\"",
    "prepare": "husky install",
    "sync": "tsx -r dotenv/config scripts/sync.ts"
  },
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@astrojs/check": "^0.3.0",
    "@astrojs/react": "^3.0.4",
    "@astrojs/rss": "^3.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/recipes": "^0.5.0",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "astro": "^3.4.2",
    "clsx": "^2.0.0",
    "downshift": "^6.1.9",
    "format-quantity": "^2.1.0",
    "lunr": "^2.3.9",
    "markdown-to-jsx": "^7.3.2",
    "match-sorter": "^6.3.1",
    "numeric-quantity": "^2.0.0",
    "param-case": "^3.0.4",
    "parse-ms": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-group": "^3.0.2",
    "remove-markdown": "^0.5.0",
    "sentence-case": "^3.0.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.2",
    "@types/lodash": "^4.14.200",
    "@types/lunr": "^2.3.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/remove-markdown": "^0.3.2",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-tamia": "^8.1.1",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-astro": "^0.12.0",
    "richtypo": "^5.1.3",
    "richtypo-rules-en": "^5.1.3",
    "tsx": "^3.14.0",
    "typescript": "5.2.2",
    "unist-util-visit": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{js,ts,tsx,astro,md}": "prettier --write",
    "*.{js,ts,tsx,astro}": "eslint --cache --fix"
  }
}
