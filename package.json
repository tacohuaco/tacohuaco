{
  "name": "tacohuaco",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "pretest": "npm run lint",
    "test": "astro check && vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:ci": "npm run lint && vitest run && astro build && npx playwright test",
    "test:e2e": "npx playwright test --ui",
    "posttest": "npm run format",
    "lint": "eslint . --cache --fix",
    "format": "prettier --log-level warn --write \"**/*.{js,cjs,mjs,ts,tsx,astro,md}\"",
    "sync": "tsx -r dotenv/config scripts/sync.ts",
    "sync:ci": "tsx -r dotenv/config scripts/syncCi.ts",
    "prepare": "husky; panda codegen"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@pandacss/dev": "^0.46.1",
    "astro": "^4.15.10",
    "downshift": "^9.0.8",
    "format-quantity": "^3.0.0",
    "lunr": "^2.3.9",
    "markdown-to-jsx": "^7.5.0",
    "match-sorter": "^6.3.4",
    "numeric-quantity": "^2.0.1",
    "param-case": "^3.0.4",
    "parse-ms": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-group": "^4.0.0",
    "remove-markdown": "^0.5.5",
    "sentence-case": "^3.0.4",
    "tamia": "^7.1.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.2",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.9",
    "@types/lunr": "^2.3.7",
    "@types/node": "^20.12.4",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@types/remove-markdown": "^0.3.4",
    "dotenv": "^16.4.5",
    "eslint": "^9.11.1",
    "eslint-config-tamia": "^9.1.23",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "graphql": "^16.9.0",
    "graphql-request": "^6.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.1",
    "mdast-util-to-markdown": "^2.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "richtypo": "^7.0.1",
    "tsx": "^4.19.1",
    "typescript": "5.6.2",
    "unist-util-visit": "^5.0.0",
    "vitest": "^2.1.1"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx,astro,md}": "prettier --write",
    "*.{js,cjs,mjs,ts,tsx,astro}": "eslint --cache --fix"
  }
}
