{
  "name": "tacohuaco",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "pretest": "npm run lint",
    "test": "astro check && vitest run",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:ci": "npm run lint && vitest run && astro build && npx playwright test",
    "test:e2e": "npx playwright test --ui",
    "posttest": "npm run format",
    "lint": "eslint . --cache --fix",
    "format": "prettier --log-level warn --write \"**/*.{js,cjs,mjs,ts,tsx,astro,md}\"",
    "sync": "node --env-file=.env --experimental-strip-types scripts/sync.ts",
    "sync:ci": "node --env-file=.env --experimental-strip-types scripts/syncCi.ts",
    "prepare": "husky; panda codegen"
  },
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.4.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "@pandacss/dev": "^1.4.2",
    "astro": "^5.14.4",
    "change-case": "^5.4.4",
    "downshift": "^9.0.10",
    "format-quantity": "^3.0.0",
    "lunr": "^2.3.9",
    "markdown-to-jsx": "^7.7.15",
    "match-sorter": "^8.1.0",
    "numeric-quantity": "^2.1.0",
    "parse-ms": "^4.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-group": "^4.0.0",
    "remove-markdown": "^0.6.2",
    "tamia": "^7.1.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.20",
    "@types/lunr": "^2.3.7",
    "@types/node": "^22.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/remove-markdown": "^0.3.4",
    "eslint": "^9.37.0",
    "eslint-config-tamia": "^9.4.3",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.3",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-to-markdown": "^2.1.2",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "richtypo": "^7.0.1",
    "typescript": "5.9.3",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx,astro,md}": "prettier --write",
    "*.{js,cjs,mjs,ts,tsx,astro}": "eslint --cache --fix"
  }
}
