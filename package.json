{
  "name": "tacohuaco",
  "private": true,
  "license": "See license in <Readme.md>",
  "scripts": {
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "pretest": "npm run lint",
    "test": "astro check && vitest run && astro build && npx playwright test",
    "test:watch": "vitest watch",
    "test:ci": "npm run lint && vitest run && astro build && npx playwright test",
    "test:e2e": "npx playwright test --ui",
    "posttest": "npm run format",
    "lint": "eslint . --cache --fix --ext .js,.ts,.tsx,.astro",
    "format": "prettier --log-level warn --write \"**/*.{js,ts,tsx,astro,md}\"",
    "sync": "tsx -r dotenv/config scripts/sync.ts",
    "sync:ci": "tsx -r dotenv/config scripts/syncCi.ts",
    "prepare": "husky install; panda codegen"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "browserslist": [
    ">1%",
    "last 1 version",
    "Firefox ESR",
    "not dead"
  ],
  "dependencies": {
    "@astrojs/check": "^0.9.3",
    "@astrojs/react": "^3.6.2",
    "@astrojs/rss": "^4.0.7",
    "@astrojs/sitemap": "^3.1.6",
    "@pandacss/dev": "^0.37.2",
    "astro": "^4.15.6",
    "downshift": "^9.0.4",
    "format-quantity": "^2.1.0",
    "lunr": "^2.3.9",
    "markdown-to-jsx": "^7.3.2",
    "match-sorter": "^6.3.1",
    "numeric-quantity": "^2.0.0",
    "param-case": "^3.0.4",
    "parse-ms": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-group": "^3.0.2",
    "remove-markdown": "^0.5.0",
    "sentence-case": "^3.0.4",
    "tamia": "^7.0.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.2",
    "@types/fs-extra": "^11.0.2",
    "@types/lodash": "^4.14.200",
    "@types/lunr": "^2.3.6",
    "@types/node": "^20.12.4",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@types/remove-markdown": "^0.3.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-config-tamia": "^8.1.3",
    "eslint-plugin-astro": "^0.34.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "lodash": "^4.17.21",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-astro": "^0.13.0",
    "richtypo": "^5.1.3",
    "richtypo-rules-en": "^5.1.3",
    "tsx": "^4.7.2",
    "typescript": "5.4.4",
    "unist-util-visit": "^5.0.0",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{js,ts,tsx,astro,md}": "prettier --write",
    "*.{js,ts,tsx,astro}": "eslint --cache --fix"
  }
}
